<p id="notice"><%= notice %></p>

<h1>Listing Objs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objs.each do |obj| %>
      <tr>
        <td><%= link_to 'Show', obj %></td>
        <td><%= link_to 'Edit', edit_obj_path(obj) %></td>
        <td><%= link_to 'Destroy', obj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obj', new_obj_path %>
